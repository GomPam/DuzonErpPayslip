{
    "manifest_version": 2,

    "name": "Lc PayPaper",
    "description": "로드컴플릿 급여명세서 복호화를 ActiveX 없이 가능하게 함.",
    "author": "GomPam - hgw3674@gmail.com",
    "version": "1.0",

    "icons" : {
        "58" : "icon.png"
    },

    "content_scripts" : [
        {
            "matches" : [
                "file:///*"
            ],
            "js" : [
                "algo/core.js",
                "algo/x64-core.js",
                "algo/enc-utf16.js",
                "algo/enc-base64.js",
                "content_script.js"
            ],
            "run_at": "document_end"
        }
    ],

    "permissions": [
        "activeTab"
    ]
}
